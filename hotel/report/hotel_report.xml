<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="action_report_room_profile" model="ir.actions.report">
        <field name="name">Room Profile</field>
        <field name="model">hotel.rooms</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">hotel.room_profile_template</field>
        <field name="report_file">hotel.room_profile_template</field>
        <field name="print_report_name">'Room profile'</field>
        <field name="binding_model_id" ref="model_hotel_rooms"/>
        <field name="binding_type">report</field>
    </record>
    <record id="acion_report_room_profile_html" model="ir.actions.report">
        <field name="name">Room Profile - HTML</field>
        <field name="model">hotel.rooms</field>
        <field name="report_type">qweb-html</field>
        <field name="report_name">hotel.room_profile_template</field>
        <field name="report_file">hotel.room_profile_template</field>
        <field name="print_report_name">'Room Profile'</field>
        <field name="binding_model_id" ref="model_hotel_rooms"/>
        <field name="binding_type">report</field>
    </record>

</odoo>